variant: fcos
version: 1.5.0
passwd:
  users:
    - name: core
      ssh_authorized_keys:
        - ssh-rsa  fabio@DevDesktop
storage:
  files:
    - path: /etc/NetworkManager/system-connections/enp0s3.nmconnection
      mode: 0600
      contents:
        inline: |
          [connection]
          id=enp0s3
          type=ethernet
          interface-name=enp0s3
          [ipv4]
          address1=192.168.56.3/24,192.168.56.1
          dhcp-hostname=coreos-pxe
          dns=1.1.1.1;
          dns-search=
          may-fail=false
          method=manual
    - path: /etc/NetworkManager/system-connections/enp0s8.nmconnection
      mode: 0600
      contents:
        inline: |
          [connection]
          id=enp0s8
          type=ethernet
          interface-name=enp0s8
          [ipv4]
          address1=192.168.57.1/24
          may-fail=false
          method=manual
